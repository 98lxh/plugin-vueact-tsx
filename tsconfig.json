{
  "include": [
    "src",
    "**/*.tsx"
  ],
  "exclude": [
    "vite"
  ],
  "compilerOptions": {
    "lib": [
      "DOM",
      "ESNext"
    ],
    "outDir": "dist",
    "target": "ES2020",
    "module": "ES2020",
    "moduleResolution": "Node",
    "strict": true,
    "declaration": true,
    "sourceMap": true,
    "noUnusedLocals": true,
    "esModuleInterop": true,
    "noImplicitAny": false,
    "jsx": "preserve",
  }
}
